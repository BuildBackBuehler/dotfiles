    {
      "fileMatch": ["intern.json", "intern-*.json", "intern.*.json"],
      "url": "https://theintern.io/intern/resources/4/schemas/config.json"
    }
  ],
  "list.source.files.defaultArgs": ["--hidden", "--unrestricted"],
  "npm.binPath": "/usr/local/bin/npm",
  "python.formatting.provider": "black",
  "scss.validate": false,
  "snippets.extends": {
    "javascriptreact": ["javascript"],
    "typescript": ["javascript"],
    "typescriptreact": ["typescript", "javascriptreact"]
