{
  "coc.preferences.currentFunctionSymbolAutoUpdate": true,
  "codeLens.enable": true,
  "diagnostic.errorSign": "",
  "diagnostic.infoSign": "",
  "diagnostic.maxWindowHeight": 20,
  "diagnostic.refreshOnInsertMode": true,
  "diagnostic.virtualText": false,
  "diagnostic.warningSign": "",
  "emmet.excludeLanguages": [
    "markdown",
    "javascript.jsx",
    "javascriptreact",
    "typescript.tsx",
    "typescriptreact"
  ],
  "explorer.sources": [
    {
      "name": "file",
      "expand": true
    }
  ],
  "explorer.icon.enableNerdfont": true,
  "explorer.file.column.git.icon.added": "✚",
  "explorer.file.column.git.icon.deleted": "✖",
  "explorer.file.column.git.icon.renamed": "➜",
  "explorer.file.column.git.icon.unmerged": "═",
  "explorer.file.column.git.icon.untracked": "?",
  "explorer.file.column.git.icon.modified": "✹",
  "explorer.file.column.git.icon.ignored": "☒",
  "explorer.file.columns": [
    "git",
    "selection",
    "clip",
    "diagnosticError",
    "indent",
    "icon",
    "filename"
  ],
  "explorer.keyMappings": {
    "<cr>": "expandOrShrink",
    "<space>": "cd"
  },
  "eslint.autoFixOnSave": true,
  "git.addGBlameToBufferVar": true,
  "git.addGBlameToVirtualText": false,
  "git.addedSign.text": "┃",
  "git.branchCharacter": "",
  "git.changeRemovedSign.text": "┃",
  "git.changedSign.text": "┃",
  "git.removedSign.text": "┃",
  "java.errors.incompleteClasspath.severity": "ignore",
  "javascript.referencesCodeLens.enable": false,
  "javascript.suggestionActions.enabled": false,
  "languageserver": {
    "ccls": {
      "command": "ccls",
      "filetypes": ["c", "cpp", "cuda", "objc", "objcpp"],
      "initializationOptions": { "cache": { "directory": ".ccls-cache" } },
      "rootPatterns": [
        ".ccls",
        "compile_commands.json",
        ".vim/",
        ".git/",
        ".hg/"
      ]
    },
    "golang": {
      "args": [],
      "command": "gopls",
      "filetypes": ["go"],
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"]
    }
  },
  "python.formatting.provider": "black",
  "python.jediEnabled": false,
  "python.linting.mypyEnabled": true,
  "suggest.enablePreview": true,
  "suggest.maxPreviewWidth": 100,
  "typescript.preferences.importModuleSpecifier": "relative",
  "typescript.referencesCodeLens.enable": false
}
